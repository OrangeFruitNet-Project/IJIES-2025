{
  "experiments": [
    {
      "name": "YOLOv4_OrangeFruitNet",
      "framework": "darknet",
      "config": {
        "dataset": "../dataset",
        "model": "yolov4",
        "imgsz": 640,
        "batch_size": 16,
        "epochs": 200,
        "optimizer": "Adam",
        "lr": 0.001,
        "beta1": 0.937,
        "beta2": 0.999,
        "scheduler": "cosine_annealing",
        "seed": 42,
        "pretrained": "../weights/yolov4_coco.weights",
        "augmentations": {
          "flip": true,
          "rotation": 15,
          "brightness": [0.85, 1.15]
        },
        "early_stop": 25,
        "workers": 4,
        "save_period": 10
      }
    },
